<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:PuzzleGraph.CustomControls"
                    >
    <Style TargetType="local:GraphNode">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:GraphNode">
                    <Grid>
                        <Ellipse Fill="AliceBlue" Width="40" Height="40" Visibility="Visible" Stroke="Black" StrokeThickness="3"></Ellipse>
                    </Grid>
                </ControlTemplate>    
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary>